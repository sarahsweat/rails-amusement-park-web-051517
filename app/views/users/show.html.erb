<h1>Hello, <%= @user.name.titleize %>!</h1>
<h2> Mood: <%= @user.mood %></h2>
<h2> Height: <%= @user.height %></h2>
<h2> Tickets: <%= @user.tickets %></h2>



<%= link_to "Log Out" , logout_path(@user), {method: "delete"}%>

<%= link_to "See attractions" , attractions_path %>

<% if flash[:notice] %>
    <div class="notice"><%= flash[:notice] %></div>
<% end %>


<% if @user.admin == true %>
    <h2> ADMIN </h2>

<% end  %>
